public class Main {
	public static void main(String args[]) {
		Product computer = new PhysicalProduct("Lenovo", 500.0d, 40);
		Product phone = new PhysicalProduct("Samsung", 540.0d, 20);
		Product e_book = new DigitalProduct("Il Signore Dei Anelli", 20.0d);
		
		Inventory inv = new Inventory();
		inv.addProduct(e_book);
		inv.addProduct(computer);
		inv.addProduct(phone);
		
		inv.viewInventory();
	
	}
}
