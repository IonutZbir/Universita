
public class PhysicalProduct implements Product{
	private String name;
	private double price;
	private int stock;
	
	public PhysicalProduct(String name, double price, int stock) {
		this.name = name;
		this.price = price; 
		this.stock = stock;
	}
	
	public void updateStock(int stock) {
		if(stock > 0) {
			this.stock += stock;
		}
	}
	
	public String getName() {
		return this.name;
	}
	public double getPrice() {
		return this.price;
	}
	
}
